{
    "name": "larabug/larabug",
    "description": "Laravel 6.x/7.x/8.x/9.x bug notifier",
    "keywords": [
        "laravel",
        "log",
        "error"
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^6.0.2 || ^7.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3 || ^1.4.2",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0 || ^7.0",
        "phpunit/phpunit": "^8.5.23 || ^9.5.12",
        "laravel/pint": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "LaraBug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraBug\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "format-dirty": "vendor/bin/pint --dirty",
        "format-dry-run": "vendor/bin/pint --test"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraBug\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis Smink",
            "email": "dennissmink@gmail.com",
            "homepage": "http://www.dennissmink.nl",
            "role": "Owner"
        }
    ]
}
