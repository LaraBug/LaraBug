{
    "name": "larabug/larabug",
    "description": "Laravel 7.x/8.x/9.x bug notifier",
    "keywords": [
        "laravel",
        "log",
        "error"
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^6.0.2 || ^7.0",
        "illuminate/support": "8.83.27 || ^9.51.0",
        "nesbot/carbon": "^2.66",
        "spatie/laravel-package-tools": "^1.12.1"
    },
    "require-dev": {
        "composer-runtime-api": "^2.0",
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^6.25.1 || ^7.22.0",
        "pestphp/pest": "^1.22.4",
        "pestphp/pest-plugin-laravel": "^1.4",
        "laravel/pint": "^1.4",
        "rector/rector": "^0.15.13"
    },
    "autoload": {
        "psr-4": {
            "LaraBug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraBug\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html build/coverage",
        "format": "vendor/bin/pint",
        "rector": "vendor/bin/rector process"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraBug\\ServiceProvider"
            ],
            "aliases": {
                "LaraBug": "LaraBug\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis Smink",
            "email": "dennissmink@gmail.com",
            "homepage": "http://www.dennissmink.nl",
            "role": "Owner"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
