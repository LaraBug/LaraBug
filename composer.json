{
    "name": "larabug/larabug",
    "description": "Laravel 7.x/8.x/9.x bug notifier",
    "keywords": [
        "laravel",
        "log",
        "error"
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "guzzlehttp/guzzle": "^6.0.2 || ^7.0",
        "illuminate/support": "^6.20.44 || ^7.30.6 || 8.83.27 || ^9.51.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^5.0 || ^6.0 || ^7.22",
        "pestphp/pest": "^1.22.4",
        "pestphp/pest-plugin-laravel": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "LaraBug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraBug\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraBug\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis Smink",
            "email": "dennissmink@gmail.com",
            "homepage": "http://www.dennissmink.nl",
            "role": "Owner"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
